[Unit]
Description=[ SN3D Bootloader ] Service
After=multi-user.target

[Service]
# The dummy program will exit
Type=oneshot

WorkingDirectory=/SN3D/sn3d-project/sn3d-bootloader
ExecStart=/SN3D/sn3d-project/sn3d-bootloader/bin/build/exe

User=root

# This service shall be considered active after start
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
